{
  "name": "konviw",
  "version": "2.10.13",
  "description": "Enterprise public viewer for your Confluence pages.",
  "author": "Jose Gascon",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && npm run scss && nest build",
    "deploy:vercel": "npm run build && vercel deploy --prod",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --list-different \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run scss && nest start --watch & npm run scss -- --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 3",
    "test": "npm run test:unit:cov && npm run test:e2e",
    "test:unit:watch": "jest --config jest-unit.config.js --watch",
    "test:unit:cov": "jest --config jest-unit.config.js --coverage",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json --runInBand --forceExit",
    "scss": "sass ./src/assets/scss:dist/static/css/ --style compressed --no-source-map",
    "cypress:run": "cypress run --headless --browser chrome",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@instana/collector": "1.138.0",
    "@nestjs/axios": "0.0.6",
    "@nestjs/common": "8.3.1",
    "@nestjs/config": "1.2.0",
    "@nestjs/core": "8.3.1",
    "@nestjs/platform-express": "8.3.1",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/swagger": "5.2.0",
    "@nestjs/terminus": "8.0.4",
    "cache-manager": "3.6.0",
    "cheerio": "1.0.0-rc.10",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "hbs": "4.2.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.4",
    "swagger-ui-express": "4.3.0",
    "tabletojson": "2.0.7"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.1",
    "@nestjs/schematics": "8.0.7",
    "@nestjs/testing": "8.3.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/cache-manager": "3.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/superagent": "4.1.15",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "cypress": "9.5.0",
    "@kiru42/cypress-plugin-visual-tests": "1.5.21",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "iframe-resizer": "4.3.2",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "sass": "1.49.9",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.7",
    "ts-node": "10.6.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.6.2"
  },
  "jest": {
    "setupFiles": [
      "./setup.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
